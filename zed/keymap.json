[
  {
    "bindings": {
      "cmd-[": "pane::GoBack",
      "cmd-]": "pane::GoForward"
    }
  },
  {
    "context": "Editor",
    "use_key_equivalents": true,
    "bindings": {
      "alt-enter": "editor::ToggleCodeActions"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "shift-enter": [
        "terminal::SendText",
        "\u001b\r"
      ]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-cmd-b": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-b": "workspace::ToggleRightDock"
    }
  }
]
